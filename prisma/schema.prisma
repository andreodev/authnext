generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId // ID autom√°tico do MongoDB
  name     String
  email    String  @unique
  password String
  image    String?
}
